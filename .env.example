# Supabase (get from project Settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Optional: server-side only
# SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# RapidAPI (for Instagram scraping)
RAPIDAPI_KEY=your-rapidapi-key

# Anthropic (for AI-assisted event parsing)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Instagram ingestion (comma-separated account usernames)
INSTAGRAM_ACCOUNTS=account1,account2

# Slack ingestion (create app at https://api.slack.com/apps)
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_CHANNEL_ID=C0123456789

# Meetup ingestion (OAuth app at https://www.meetup.com/api/oauth/)
MEETUP_CLIENT_ID=your-meetup-client-id
MEETUP_CLIENT_SECRET=your-meetup-client-secret
MEETUP_REFRESH_TOKEN=your-meetup-refresh-token

# Cron job security (used by Vercel cron to authenticate requests)
CRON_SECRET=your-cron-secret
